{
  "name": "@jjordy/datagrid",
  "version": "1.0.91",
  "description": "Composable Datagrid for react",
  "main": "./lib/index.js",
  "module": "./lib/es/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "node ./tools/build.js",
    "dev": "babel ./src -d lib/es --ignore __tests__ --watch",
    "lint": "eslint src test",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jjordy/gfas-react-components.git"
  },
  "author": "Jordan Addison <jordanrileyaddison@gmail.com> (http://github.com/jjordy)",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "15 || 16",
    "styled-components": "3 || 4"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "eslint": "^5.4.0",
    "jest": "^23.5.0"
  },
  "dependencies": {
    "@jjordy/layout": "^0.0.92",
    "date-fns": "^1.29.0",
    "dom-helpers": "^3.2.1",
    "fuse.js": "^3.2.0",
    "lodash.sortby": "^4.7.0",
    "react-virtualized": "^9.20.1",
    "uuid": "^3.2.1"
  }
}
