{
  "name": "@jjordy/datagrid",
  "version": "1.1.4",
  "description": "Composable Datagrid for react",
  "main": "./lib/index.js",
  "module": "./lib/es/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "node ./tools/build.js",
    "dev": "babel ./src -d lib/es --ignore __tests__ --watch",
    "lint": "eslint src test",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/testSetup.js"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "suiteName": "@jjordy/datagrid - Tests",
          "outputDirectory": "../../test-results/jest/datagrid",
          "outputName": "results.xml"
        }
      ]
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jjordy/gfas-react-components.git"
  },
  "author": "Jordan Addison <jordanrileyaddison@gmail.com> (http://github.com/jjordy)",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "15 || 16",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.6.0",
    "@babel/core": "^7.6.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.6.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.6.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/preset-env": "^7.6.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^26.0.1",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "jest-junit": "^10.0.0",
    "mkdirp": "^1.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "rimraf": "^3.0.0"
  },
  "dependencies": {
    "@jjordy/layout": "^1.1.4",
    "date-fns": "1",
    "dom-helpers": "^5.1.0",
    "fuse.js": "^6.0.0",
    "lodash.sortby": "^4.7.0",
    "react-virtualized": "^9.20.1",
    "uuid": "^8.1.0"
  }
}
