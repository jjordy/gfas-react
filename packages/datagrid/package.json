{
  "name": "@jjordy/datagrid",
  "version": "1.1.0",
  "description": "Composable Datagrid for react",
  "main": "./lib/index.js",
  "module": "./lib/es/index.js",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "node ./tools/build.js",
    "dev": "babel ./src -d lib/es --ignore __tests__ --watch",
    "lint": "eslint src test",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/testSetup.js"
    ],
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "suiteName": "@jjordy/datagrid - Tests",
          "outputDirectory": "../../test-results/datagrid",
          "outputName": "junit.xml"
        }
      ]
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jjordy/gfas-react-components.git"
  },
  "author": "Jordan Addison <jordanrileyaddison@gmail.com> (http://github.com/jjordy)",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "^15.6.2",
    "react": "15 || 16",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^5.4.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-config-standard-react": "^7.0.2",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^8.0.0",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.1.0",
    "jest-junit": "^6.3.0",
    "mkdirp": "^0.5.1",
    "prettier": "^1.10.2",
    "pretty-quick": "^1.2.2",
    "react": "16.8.4",
    "react-dom": "16.8.4",
    "rimraf": "^2.6.2"
  },
  "dependencies": {
    "@jjordy/layout": "^1.1.0",
    "date-fns": "^1.29.0",
    "dom-helpers": "^3.2.1",
    "fuse.js": "^3.2.0",
    "lodash.sortby": "^4.7.0",
    "react-virtualized": "^9.20.1",
    "uuid": "^3.2.1"
  }
}
