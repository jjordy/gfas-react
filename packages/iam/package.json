{
  "name": "@jjordy/iam",
  "version": "1.0.39",
  "description": "React Utilities for Dealing with IAM Policies",
  "main": "./lib/index.js",
  "module": "./lib/es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "node ./tools/build.js",
    "dev": "babel ./src -d lib/es --ignore __tests__ --watch",
    "lint": "eslint src test",
    "prepublishOnly": "npm run build",
    "test": "jest"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "collectCoverage": true,
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/testSetup.js"
    ],
    "coveragePathIgnorePatterns": [
      "/test/"
    ],
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jjordy/gfas-IAM-react.git"
  },
  "author": "Jordan Addison <jordanrileyaddison@gmail.com> (http://github.com/jjordy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jjordy/gfas-IAM-react.git"
  },
  "homepage": "https://github.com/jjordy/gfas-IAM-react",
  "peerDependencies": {
    "prop-types": "15 || 16",
    "react": "15 || 16",
    "react-dom": "15 || 16"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.3",
    "eslint": "^5.4.0",
    "jest": "^23.5.0"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0-rc.3",
    "deep-equal": "^1.0.1",
    "validate.js": "^0.12.0"
  }
}
