{
  "name": "@jjordy/vtable",
  "version": "0.0.1",
  "description": "Virtual Table Component",
  "main": "./lib/index.js",
  "module": "./lib/es/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "node ./tools/build.js",
    "dev": "babel ./src -d lib/es --ignore __tests__ --watch",
    "lint": "eslint src test --fix",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jjordy/vtable.git"
  },
  "author": "Jordan Addison <jordanrileyaddison@gmail.com> (http://github.com/jjordy)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jjordy/vtable/issues"
  },
  "homepage": "https://github.com/jjordy/vtable",
  "peerDependencies": {
    "prop-types": "15 || 16",
    "react": "15 || 16",
    "react-dom": "15 || 16",
    "styled-components": "^4.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "babel-plugin-styled-components": "^1.8.0",
    "eslint": "^5.4.0",
    "jest": "^23.5.0",
    "react-test-render": "^1.1.1"
  },
  "dependencies": {
    "react-window": "^1.2.4"
  }
}
