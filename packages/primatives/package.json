{
  "name": "@jjordy/primatives",
  "version": "0.0.1",
  "description": "Composable layout primatives",
  "main": "./lib/index.js",
  "module": "./lib/es/index.js",
  "typings": "./lib/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "tsc -p tsconfig.base.json",
    "dev": "tsc -w -p tsconfig.base.json",
    "lint": "eslint src test --fix",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/test/shim.js",
      "<rootDir>/test/testSetup.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/lib"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jjordy/@jjordy/primatives"
  },
  "author": "Jordan Addison <jordanrileyaddison@gmail.com> (http://github.com/jjordy)",
  "license": "MIT",
  "peerDependencies": {
    "react": "15 || 16",
    "react-dom": "15 || 16",
    "styled-components": "3 || 4"
  },
  "devDependencies": {
    "@types/color": "^3.0.0",
    "@types/jest": "^23.3.7",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/styled-components": "^4.0.2",
    "jest": "^23.5.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "color": "^3.1.0"
  }
}
